{
    "name": "steff81/translation-bundle",
    "type": "symfony-bundle",
    "description": "Translation bundle for Symfony2 and Doctrine 2",
    "keywords": ["orm", "doctrine", "translation"],
    "homepage": "https://github.com/Steff81/TranslationBundle",
    "version": "0.1",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Matteo Giachino",
            "email": "matteog@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "doctrine/doctrine-bundle": ">=1.1",
        "doctrine/orm": "2.2.*"
    },
    "require-dev": {
        "twig/twig": "1.9.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "Steff81\\TranslationBundle": "" }
    },
    "target-dir": "Steff81/TranslationBundle"
}
